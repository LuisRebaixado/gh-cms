    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "jest": "^28.1.0",
